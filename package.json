{
  "name": "handy-kk-server",
  "version": "1.0.0-alpha",
  "description": "A complete Node.js project template using TypeScript and following general best practices.",
  "keywords": [
    "thehandy",
    "koikatsu",
    "koikatu"
  ],
  "author": {
    "name": "Karil Chan",
    "url": "https://www.karil.rs/"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriswells0/node-typescript-template.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "changelog": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "clean": "(rm -r ./.nyc_output || true) && (rm -r ./coverage || true) && (rm -r ./dist || true)",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "cover": "nyc npm run test",
    "cover:check": "nyc --check-coverage npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/**/*.ts",
    "preversion": "npm run build && npm run cover:check",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "test": "mocha",
    "upgrade": "npx npm-check -u",
    "version": "npm run build && npm run changelog:update"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.9",
    "@types/ping": "^0.2.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^7.23.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^8.1.3",
    "nyc": "^15.0.0",
    "ping": "^0.4.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^9.0.0",
    "tslint": "^6.0.0",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "json-2-csv": "^3.10.3",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sitka": "^1.0.5",
    "stdout-update": "^1.6.5",
    "thehandy": "github:KarilChan/thehandy",
    "webpack": "^5.34.0",
    "winston": "^3.3.3",
    "yargs": "^15.3.1"
  },
  "proxy": "http://localhost:42069"
}
